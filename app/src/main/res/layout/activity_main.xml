<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/mainFrameLayout"
    tools:context=".MainActivity">
    <include layout="@layout/layout_menu"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="300"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="100"
            android:weightSum="100"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="60"
                android:weightSum="100"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="40"
                    android:weightSum="100"
                    android:orientation="horizontal">

                    <Space
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="35"
                        tools:ignore="NestedWeights" />

                    <ImageButton
                        android:id="@+id/imageButton0"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="30"
                        android:contentDescription="Logo"
                        app:srcCompat="@drawable/temp_button_75"/>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="35">

                        <ImageButton
                            android:id="@+id/imageButton1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:contentDescription="Menu"
                            android:background="@drawable/image_button_menu"
                            app:srcCompat="@drawable/temp_button_50"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintVertical_bias="0.5"
                            app:layout_constraintHorizontal_bias="0.5"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:weightSum="100"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="60"
                        android:weightSum="100"
                        android:orientation="horizontal">

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="5" />

                        <EditText
                            android:id="@+id/searchEditText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="80"
                            android:layout_marginTop="16dp"
                            android:ems="10"
                            android:inputType="text"
                            android:minHeight="48dp"
                            android:background="@drawable/edit_text_search"
                            android:padding="8dp"
                            android:hint="Text"
                            android:textColor="#AAAAAA"
                            android:autofillHints="" />

                        <Space
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="5" />


                        <ImageButton
                            android:id="@+id/imageButton14"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginTop="16dp"
                            app:srcCompat="@drawable/temp_button_50" />

                        <Space
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="5" />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="40"
                android:weightSum="100"
                android:orientation="horizontal">

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />

                <TableLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="80"
                    android:background="@drawable/table_layout_main"
                    android:weightSum="110">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="30"
                        android:weightSum="40">

                        <ImageButton
                            android:id="@+id/imageButton2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/korean"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton3"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/western"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton4"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/chinese"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton5"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/japanese"
                            app:srcCompat="@drawable/temp_button_50"/>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_marginTop="10dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="30"
                        android:weightSum="40">

                        <ImageButton
                            android:id="@+id/imageButton6"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/asian"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton7"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/chicken"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton8"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/pizza"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton9"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/fastFood"
                            app:srcCompat="@drawable/temp_button_50"/>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="30"
                        android:weightSum="40"
                        android:layout_marginTop="10dp">

                        <ImageButton
                            android:id="@+id/imageButton10"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/bunsik"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton11"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/bar"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton12"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/cafe"
                            app:srcCompat="@drawable/temp_button_50"/>

                        <ImageButton
                            android:id="@+id/imageButton13"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="10"
                            android:background="@drawable/image_button_food_type"
                            android:scaleType="centerInside"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:contentDescription="@string/dessert"
                            app:srcCompat="@drawable/temp_button_50"/>
                    </TableRow>
                </TableLayout>

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="200"
            android:weightSum="100"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="75"
                android:weightSum="100"
                android:orientation="horizontal">

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />

                <Button
                    android:id="@+id/reviewButton"
                    style="@style/MyButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:layout_weight="80"
                    android:text="@string/review"
                    android:textColor="#222222"
                    app:cornerRadius="12dp"
                    tools:ignore="TouchTargetSizeCheck"
                    android:textAllCaps="false"/>

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="10" />
            </LinearLayout>

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="25"
                android:contentDescription="다른사람의 리류를 볼 수 있는 스크롤뷰">

                <LinearLayout
                    android:id="@+id/linearLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:contentDescription="review layout">
                </LinearLayout>
            </HorizontalScrollView>

        </LinearLayout>

    </LinearLayout>
</FrameLayout>